{
  "name": "indovendor-monorepo",
  "version": "1.0.0",
  "description": "IndoVendor EO/WO Marketplace - Monorepo",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "lint:all": "cd backend && npm run lint && cd ../frontend && npm run lint",
    "format:backend": "cd backend && npm run format",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules backend/dist frontend/.next"
  },
  "keywords": [
    "marketplace",
    "event-organizer",
    "wedding-organizer",
    "indonesia",
    "nextjs",
    "express",
    "typescript"
  ],
  "author": "Muhammad Seman",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  }
}